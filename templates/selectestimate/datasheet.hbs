<div id="tableContainer">
	<table data-role="table" id="tableItems" class="ui-body-d ui-shadow table-stripe ui-responsive">
	  <thead>
		  <tr>
		    <th colspan="2">NAME OF THE WORK:</th>
		    <th colspan="5">Testlakjsdfl alskjdflaksjdf</th>
		  </tr>
		  <tr>
		    <th colspan="4"></th>
		    <th colspan="3">ESt.Cost Rs:1.00Lakhs</th>
		  </tr>
		  <tr>
		    <th colspan="2">Quantity</th>
		    <th>Description</th>
		    <th>Rate</th>
		    <th colspan="2">Per</th>
		    <th>Amount</th>
		  </tr>
	  </thead>
	  <tbody>
	  {{#each codeToDatas}}
	  	{{#each this.datas}}
	  		{{#compare @index 0 operator="=="}}
		      <tr>
		      	<td><strong>{{IndexCode}}</strong></td>
		  		<td><strong>{{add ../../itemIndex 1}}</strong></td>
		  		<td colspan="2"><strong>{{Description}}</strong></td>
		  		<td></td>
		  		<td></td>
		  		<td></td>	
		  	  </tr>		
	  		{{else}}
	  		  {{#isNull SubBullet}}
				  <tr>
				    <td>{{Quantity}}</td>
				    <td>{{Unit}}</td>
				    {{#compare descType "Normal" operator="=="}}
				    	<td>{{Description}}</td>
				    {{else}}
				    	{{#compare descType "RateOrCost" operator="=="}}
					    	<td style="color:red"><strong>{{Description}}</strong></td>
					    {{else}}
					    	<td><strong>{{Description}}</strong></td>				    	
					    {{/compare}}			    
				    {{/compare}}
				    	<td>{{RateRs}}</td>
				    {{#isNull Unit}}
				    	<td></td>
				    {{else}}
				    	<td>1</td>
				    {{/isNull}}
				    <td>{{Unit}}</td>
				    {{#isNull Amount}}
				    	<td>{{Amount}}</td>
				    {{else}}
				    	<td>{{decimalFormat Amount}}</td>  
				    {{/isNull}}
				  </tr>
			  {{else}}
				 <tr>
				    <td>{{Quantity}}</td>
				    <td><strong>{{SubBullet}}</strong></td>
				    <td><strong>{{Description}}</strong></td>
				     <td>{{RateRs}}</td>
				    <td></td>
				    <td>{{Unit}}</td>
				    <td></td>
				  </tr>		  		  
			  {{/isNull}}
		  	{{/compare}}
	  	{{/each}}
	  {{/each}}
	  </tbody>
	</table>
	<div class="footer-buttons">
		<hr />
		<div class="ui-grid-a">
			<div class="ui-block-a footer-left">
				<input type="button" value="Clear Selection" data-inline="true"/>
			</div>
			<div class="ui-block-b footer-right">
				<input type="button" data-inline="true" data-icon="arrow-r" data-iconpos="right" value="PROCEED TO ABSTRACT ESTIMATE" id="abstractEstimate"/>
			</div>
		</div>
	</div>
</div>
